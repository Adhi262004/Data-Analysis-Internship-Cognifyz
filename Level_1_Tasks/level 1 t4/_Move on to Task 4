    "df_clean = df.dropna(subset=[\"Has Online delivery\", \"Aggregate rating\"])\n",
    "avg_rating_comparison = (\n",
    "    df_clean.groupby(\"Has Online delivery\")[\"Aggregate rating\"]\n",
    "    .mean()\n",
    "    .round(2)\n",
    "    .reset_index()\n",
    "    .rename(columns={\"Aggregate rating\": \"Average Rating\"})\n",
    ")\n",
    "\n",
    "print(\"\\n Average Ratings by Online Delivery Option:\")\n",
    "print(avg_rating_comparison)"
